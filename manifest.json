{
     "manifest_version": 3,
     "name": "Hack",
     "description": "Hackathon Extension",
     "version": "1.0",
     "permissions": [
        "activeTab",
        "scripting"
      ],
     "action": {
          "default_popup": "hello.html",
          "default_icon": "/images/emoji.png",
          "default_title": "You are getting hacked!"
     },
     "content_scripts": [
          {
               "matches": ["*://*/*"],
               "js": ["content.js"]
          }
     ]
}
